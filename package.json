{
  "name": "b2b_console",
  "version": "1.0.0",
  "description": "b2b console with vue/nuxt",
  "author": "kevin <wanghuan@silvrr.com>",
  "private": true,
  "config": {
    "nuxt": {
      "host": "0.0.0.0",
      "port": "3330"
    }
  },
  "scripts": {
    "dev": "npm run zh && nuxt",
    "server": "cross-env server=true nodemon --exec babel-node ./server -w ./server",
    "build": "npm run zh && nuxt build",
    "start": "nuxt start",
    "pm2": "pm2 start npm --name b2b_pc -- start && pm2 start npm --name b2b_pc -- server",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "zh": "node ./plugins/i18n_zh.js",
    "precommit": "npm run lint"
  },
  "dependencies": {
    "@cjy0208/vue-i18n": "^7.7.1",
    "@koa/cors": "^2.2.1",
    "awe-dnd": "^0.3.1",
    "axios": "^0.18.0",
    "dayjs": "^1.5.24",
    "echarts": "^4.1.0",
    "element-ui": "^2.2.2",
    "formidable": "^1.1.1",
    "highlight.js": "^9.12.0",
    "jsonwebtoken": "^8.1.1",
    "koa": "^2.4.1",
    "koa-bodyparser": "^4.2.0",
    "koa-formidable": "^1.1.0",
    "koa-router": "^7.3.0",
    "koa-session": "^5.8.1",
    "koa-session-mongoose": "^2.0.5",
    "koa-static": "^4.0.2",
    "markdown-it": "^8.4.0",
    "md5": "^2.2.1",
    "mongoose": "^5.0.1",
    "nuxt": "^1.0.0",
    "postcss-cssnext": "^3.0.2",
    "postcss-import": "^11.0.0",
    "postcss-url": "^7.3.0",
    "source-map-support": "^0.4.15",
    "top-editor": "0.0.7",
    "vue-i18n": "^7.8.0",
    "vuedraggable": "^2.16.0",
    "vuex-persistedstate": "^2.5.1",
    "xlsx": "^0.13.3"
  },
  "devDependencies": {
    "@nuxtjs/axios": "^5.3.1",
    "@nuxtjs/proxy": "^1.2.4",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-preset-latest-node": "^0.4.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "backpack-core": "^0.3.0",
    "cross-env": "^5.1.4",
    "eslint": "^4.15.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-html": "^3.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-node": "^4.2.2",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-standard": "^3.0.1",
    "eslint-plugin-vue": "^4.0.0",
    "global-tunnel": "^1.2.0",
    "node-sass": "^4.8.3",
    "nodemon": "^1.11.0",
    "sass-loader": "^6.0.7",
    "scmp": "^2.0.0"
  }
}
